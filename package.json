{
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@babel/preset-env": "^7.19.4",
    "cypress": "^13.15.0",
    "eslint": "^8.57.1",
    "eslint-plugin-cypress": "^4.0.0",
    "eslint-plugin-jest": "^28.8.3",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "scripts": {
    "lint": "eslint src/js/api/auth/tests.test.js",
    "lint-fix": "eslint src/js/api/auth/tests.test.js --cache --fix",
    "prepare": "husky install",
    "test": "jest --coverage",
    "test-e2e": "cypress open",
    "test-e2e:run": "cypress run",
    "test:all": "npm run test && npm run test-e2e:run",
    "dev": "webpack serve --mode development"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  }
}
